{
    "cmd": 
    [
        "g++",
        "-std=c++17",
        "-Wall",
        "-Wextra",
        "-pedantic",     
        "-O2",
        "-Wshadow",
        "-Wformat=2",
        "-Wfloat-equal", 
        "-Wconversion",  
        "-Wlogical-op",
        "-Wshift-overflow=2",
        "-Wduplicated-cond",
        "-Wcast-qual",
        "-Wcast-align",
        "-D_FORTIFY_SOURCE=2",
        "-fstack-protector",
        "$file",
        "-o",
        "a.exe",
        "&&",
        "a.exe",
        "<",
        "D:\\Coding\\Competitive-Programming\\data\\input.txt",
        ">",
        "D:\\Coding\\Competitive-Programming\\data\\output.txt"
    ],
    "shell": true,
    "file_regex": "^(.*):([0-9]+):([0-9]+): error: (.*)$",
    "working_dir": "$file_path",
    "selector": "source.c++",
}